{
    "manifest_version": 2,

    "name": "TOC X",
    "description": "This extension create table of contents",
    "version": "0.1",

    "icons": {
        "32": "img/icon32.png",
        "64": "img/icon64.png"
    },

    "browser_action": {
        "default_icon": "img/icon64.png",
        "default_popup": "test.html",
        "default_title": "配置TOCX"
    },
    "content_scripts": [{
        "js": ["toc.js"],
        "matches": [
            "http://*/*",
            "https://*/*"
        ],
        "run_at": "document_end"
    }],
    "permissions": [
        "http://*/*",
        "https://*/*"
    ]
}
